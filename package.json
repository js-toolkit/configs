{
  "name": "@vzh/configs",
  "version": "1.28.0",
  "description": "Default configurations for internal projects",
  "main": "./",
  "scripts": {
    "clean": "rimraf lib/",
    "build": "npm run clean && babel src/ --out-dir lib/ --copy-files && cp -t ./lib/ package.json LICENSE",
    "build-to-project": "npm run clean && babel src/ --out-dir '../project-langlearn/node_modules/@vzh/configs/' --copy-files",
    "build-to-reflexy": "npm run clean && babel src/ --out-dir '../reflexy/node_modules/@vzh/configs/' --copy-files",
    "build-to-js-collections": "npm run clean && babel src/ --out-dir '../js-collections/node_modules/@vzh/configs/' --copy-files",
    "build-to-tabix": "npm run clean && babel src/ --out-dir '../tabixio/tabix/node_modules/@vzh/configs/' --copy-files",
    "patch-publish": "npm version patch && npm run build && npm publish ./lib --access public && git push --follow-tags",
    "minor-publish": "npm version minor && npm run build && npm publish ./lib --access public && git push --follow-tags",
    "major-publish": "npm version major && npm run build && npm publish ./lib --access public && git push --follow-tags"
  },
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "contributors": [
    {
      "name": "Vladimir Zhukov",
      "url": "http://github.com/vlazh"
    }
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "babel-eslint": "^8.2.3",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.0",
    "mini-css-extract-plugin": "^0.4.0",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "tsconfig-paths-webpack-plugin": "^3.1.4",
    "webpack": "^4.16.5",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
