{
  "name": "@vzh/configs",
  "version": "3.56.0",
  "description": "Preconfigured configurations",
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "main": "./",
  "scripts": {
    "update-deps": "yarn upgrade-interactive --latest",
    "clean": "rimraf dist/*",
    "copy:configs": "copyfiles package.json .npmignore README.md LICENSE ./dist/",
    "copy": "copyfiles --exclude './src/**/*.ts' --up 1 './src/**/*' ./dist/ && npm run copy:configs",
    "build": "npm run clean && tsc --project ./tsconfig.json",
    "build-to-tutoron": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/configs/' && copyfiles --exclude './src/**/*.ts' --up 1 './src/**/*' '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/configs/'",
    "build-to-reflexy": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/reflexy/node_modules/@vzh/configs/' && copyfiles --exclude './src/**/*.ts' --up 1 './src/**/*' '/home/vladimir/dev/sources/reflexy/node_modules/@vzh/configs/'",
    "build-to-react-components": "tsc --project ./tsconfig.json --outDir '../react-components/node_modules/@vzh/configs/' && copyfiles --exclude './src/**/*.ts' --up 1 './src/**/*' '../react-components/node_modules/@vzh/configs/'",
    "build-to-you.fan": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/you.fan/node_modules/@vzh/configs' && copyfiles --exclude './src/**/*.ts' --up 1 './src/**/*' '/home/vladimir/dev/sources/you.fan/node_modules/@vzh/configs'",
    "patch-publish": "npm run build && npm version patch && npm run copy && npm publish ./dist --access public && git push --follow-tags",
    "minor-publish": "npm run build && npm version minor && npm run copy && npm publish ./dist --access public && git push --follow-tags",
    "major-publish": "npm run build && npm version major && npm run copy && npm publish ./dist --access public && git push --follow-tags"
  },
  "devDependencies": {
    "@types/eslint": "^6.8.1",
    "@types/node": "^14.0.1",
    "@types/webpack": "4.41.13",
    "@types/webpack-dev-server": "^3.10.1",
    "@types/webpack-node-externals": "^1.7.1",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "copyfiles": "^2.3.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jss": "^10.1.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {}
}
