{
  "name": "@vlazh/configs",
  "version": "3.73.8",
  "description": "Preconfigured configurations",
  "author": "VZH",
  "license": "MIT",
  "main": "./",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/configs"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "copy:configs": "copyfiles package.json .npmignore README.md LICENSE ./dist/",
    "copy": "copyfiles --exclude './src/**/*.ts' --up 1 './src/**/*' ./dist/ && npm run copy:configs",
    "build": "npm run clean && tsc --project ./tsconfig.json",
    "build-to-tutoron": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/configs/' && copyfiles --exclude './src/**/*.ts' --up 1 './src/**/*' '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/configs/'",
    "build-to-reflexy": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/reflexy/node_modules/@vzh/configs/' && copyfiles --exclude './src/**/*.ts' --up 1 './src/**/*' '/home/vladimir/dev/sources/reflexy/node_modules/@vzh/configs/'",
    "build-to-react-components": "tsc --project ./tsconfig.json --outDir '../react-components/node_modules/@vzh/configs/' && copyfiles --exclude './src/**/*.ts' --up 1 './src/**/*' '../react-components/node_modules/@vzh/configs/'",
    "build-to-player": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/player/node_modules/@vzh/configs' && copyfiles --exclude './src/**/*.ts' --up 1 './src/**/*' '/home/vladimir/dev/sources/player/node_modules/@vzh/configs'",
    "build-to-player-old": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/player-old/node_modules/@vzh/configs' && copyfiles --exclude './src/**/*.ts' --up 1 './src/**/*' '/home/vladimir/dev/sources/player-old/node_modules/@vzh/configs'",
    "patch-publish": "npm run build && npm version patch && npm run copy && npm publish ./dist && git push --follow-tags",
    "minor-publish": "npm run build && npm version minor && npm run copy && npm publish ./dist && git push --follow-tags",
    "major-publish": "npm run build && npm version major && npm run copy && npm publish ./dist && git push --follow-tags"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/node": "^14.14.28",
    "@types/webpack-dev-server": "^3.11.1",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "copyfiles": "^2.4.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "html-webpack-plugin": "^5.2.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.5",
    "webpack": "^5.24.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {}
}
