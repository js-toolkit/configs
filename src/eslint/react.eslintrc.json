{
  "extends": [
    "./common.eslintrc.json",
    "airbnb",
    "plugin:prettier/recommended",
    "plugin:css-modules/recommended",
    "./base.rules.eslintrc.json"
  ],
  "plugins": ["react", "jsx-a11y", "css-modules"],
  "env": {
    "browser": true,
    "node": false
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
