{
  "extends": ["./common.eslintrc.json", "./ts.base.rules.eslintrc.json"],
  "parser": "typescript-eslint-parser",
  "plugins": ["typescript"],
  "rules": {
    "class-methods-use-this": "off",
    "no-use-before-define": "off",
    "import/no-unresolved": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      { "js": "never", "jsx": "never", "ts": "never", "tsx": "never" }
    ]
  }
}
