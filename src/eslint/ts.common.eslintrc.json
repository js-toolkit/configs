{
  "parser": "typescript-eslint-parser",
  "extends": ["./common.eslintrc.json", "./ts.base.rules.eslintrc.json"],
  "plugins": ["typescript"],
  "rules": {
    "import/no-unresolved": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      { "js": "never", "jsx": "never", "ts": "never", "tsx": "never" }
    ]
  }
}
