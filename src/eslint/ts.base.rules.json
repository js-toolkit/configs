{
  "rules": {
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-use-before-define": "off",
    "no-restricted-globals": "off",
    "no-redeclare": "off",
    "no-inner-declarations": ["off", "functions"],
    "no-useless-constructor": "off",
    "no-empty-function": ["error", { "allow": ["constructors"] }],
    "class-methods-use-this": "off"
  }
}
